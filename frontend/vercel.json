{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://stockmarket-olld.vercel.app"
  },
  "installCommand": "cd frontend && npm install",
  "buildCommand": "cd frontend && npm run build"
}
